.ng-close-tooltip { color: rgb(255, 255, 255); font-size: 12px; cursor: pointer; }

.close-tooltip { margin-top: 4px; margin-left: 8px; margin-right: 2px; }

.ng-close-tooltip::before { color: rgb(255, 255, 255); font-weight: 600; }

.title-container { display: flex; flex-direction: row-reverse; padding-top: 10px !important; padding-left: 30px !important; }

.w-max-content { width: max-content !important; }

.glow-on-hover { z-index: 0; }

.px-96 { padding-right: 96px; padding-left: 96px; }

.quote::before { content: ""; background-image: url("https://cdn.karlancer.com/assets/content-landing/quote-96.png"); width: 96px; height: 96px; display: block; position: absolute; margin-right: -96px; margin-top: -31px; }

.glow-on-hover::before { content: ""; background-image: ; background-position-x: ; background-position-y: ; background-repeat-x: ; background-repeat-y: ; background-attachment: ; background-origin: ; background-clip: ; background-color: ; position: absolute; top: -2px; left: -2px; background-size: 400%; z-index: -1; filter: blur(5px); width: calc(100% + 4px); height: calc(100% + 4px); animation: 20s linear 0s infinite alternate none running glowing; opacity: 1; transition: opacity 0.3s ease-in-out 0s; border-radius: 10px; }

.glow-on-hover:active { color: rgb(0, 0, 0); }

.sticky-bottom { position: sticky; right: 0px; bottom: 0px; left: 0px; z-index: 1030; }

@keyframes glowing { 
  0% { background-position: 0px 0px; }
  50% { background-position: 400% 0px; }
  100% { background-position: 0px 0px; }
}

